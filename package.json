{
  "name": "lucid-web-api",
  "description": "A framework for building web APIs with TypeScript",
  "version": "0.0.14",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts",
    "prepublish": "npm run typings install && npm run build",
    "test": "mocha tests/**/*.spec.ts --require ts-node/register",
    "typings": "typings"
  },
  "author": "Luke Autry <lukeautry@gmail.com> (http://www.lukeautry.com)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "typescript": "^1.8.10",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "ts-node": "^0.9.3",
    "tslint": "^3.11.0",
    "typings": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeautry/lucid-web-api.git"
  },
  "bin": {
    "lucid-swagger-generate": "dist/swagger/cli.js"
  }
}
